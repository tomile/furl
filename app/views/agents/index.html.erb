<p id="notice"><%= notice %></p>

<h1>Listing Agents</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Middle name</th>
      <th>Last name</th>
      <th>Picture</th>
      <th>Bio</th>
      <th>Phone</th>
      <th>Email</th>
      <th>Website</th>
      <th>Count click to contact</th>
      <th>Count click to accept</th>
      <th>Count closed</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @agents.each do |agent| %>
      <tr>
        <td><%= agent.first_name %></td>
        <td><%= agent.middle_name %></td>
        <td><%= agent.last_name %></td>
        <td><%= agent.picture %></td>
        <td><%= agent.bio %></td>
        <td><%= agent.phone %></td>
        <td><%= agent.email %></td>
        <td><%= agent.website %></td>
        <td><%= agent.count_click_to_contact %></td>
        <td><%= agent.count_click_to_accept %></td>
        <td><%= agent.count_closed %></td>
        <td><%= link_to 'Show', agent %></td>
        <td><%= link_to 'Edit', edit_agent_path(agent) %></td>
        <td><%= link_to 'Destroy', agent, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Agent', new_agent_path %>
