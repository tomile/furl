<p id="notice"><%= notice %></p>

<h1>Listing Prospects</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Middle name</th>
      <th>Last name</th>
      <th>Email</th>
      <th>Phone</th>
      <th>Notes</th>
      <th>Zip</th>
      <th>Agent</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @prospects.each do |prospect| %>
      <tr>
        <td><%= prospect.first_name %></td>
        <td><%= prospect.middle_name %></td>
        <td><%= prospect.last_name %></td>
        <td><%= prospect.email %></td>
        <td><%= prospect.phone %></td>
        <td><%= prospect.notes %></td>
        <td><%= prospect.zip %></td>
        <td><%= prospect.agent %></td>
        <td><%= link_to 'Show', prospect %></td>
        <td><%= link_to 'Edit', edit_prospect_path(prospect) %></td>
        <td><%= link_to 'Destroy', prospect, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Prospect', new_prospect_path %>
